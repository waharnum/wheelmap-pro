{
  "extends": [
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "ordered-imports": false,
    "no-var-requires": false,
    "jsx-no-multiline-js": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "semicolon": [
      true,
      "always"
    ]
  }
}
